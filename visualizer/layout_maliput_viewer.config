<?xml version="1.0"?>

<window>
  <state></state>
  <position_x>278</position_x>
  <position_y>172</position_y>
  <width>1328</width>
  <height>730</height>
  <ignore>position</ignore>
  <stylesheet>
    /* We need an empty stylesheet so that it loads the system's default style */
  </stylesheet>
  <menus>
    <plugins visible="false" />
  </menus>
</window>

<plugin filename="MaliputViewerWidget">
  <has_titlebar>false</has_titlebar>
</plugin>

<plugin filename="Grid3D" >
  <insert>
    <cell_count>50</cell_count>
  </insert>
</plugin>

<!-- Time panel -->
<plugin filename="TimePanel">
  <title>Time</title>

  <world_control>
    <play_pause>true</play_pause>
    <start_paused>false</start_paused>
    <service>/world_control</service>
  </world_control>

  <world_stats>
    <sim_time>true</sim_time>
    <real_time>true</real_time>
    <topic>/world_stats</topic>
  </world_stats>
</plugin>
